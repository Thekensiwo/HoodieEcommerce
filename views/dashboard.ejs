                <h3>Revenue</h3>
                
                <div class="stats">
                    <div>Today: <span><%= today %></span></div>
                    <div>This Month: <span><%= month %></span></div>
                    <div>Total: <span><%= total %></span></div>
                </div>
            </div>

            <div class="last-orders-cont">
                <h4 class="last-orders-h4">Last orders</h4>
                <div class="last-orders">

                    <table>

                        <tr class="table-header">
                            <th class="th-id">Id</th>
                            <th class="th-us">Username</th>
                            <th class="th-em">Email</th>
                            <th class="th-it">Items</th>
                            <th class="th-pr">Price</th>
                            <th class="th-det">Details</th>
                        </tr>

                        <% lastOrders.forEach(item =>{ %>
                            <% 
                                let fullName = item.user.name + ' ' + item.user.lastName
                                let shortEmail = item.user.email;

                                if(fullName.length > 20){
                                    fullName = fullName.slice(0,20) + '...'
                                }
                                if(item.user.email.length > 20)
                                    shortEmail = item.user.email.slice(0,20) + '...'
                            %>
                            <tr>
                                <td class="order-id">1</td>
                                <td title="<%= item.user.name + ' ' + item.user.lastName %>"><%= fullName %></td>
                                <td title="<%= item.user.email %>"><%= shortEmail %></td>
                                <td class="order-items-cont">
                                    <div class="order-items-inner">
                                        <div class="order-items">
                                            <% item.items.forEach(it =>{ %>
                                                <% console.log("it:", it) %>
                                                <div class="order-item">
                                                    <img src="<%= it.mainImg %>">
                                                </div>
                                            <% }) %>
                                        </div>
                                    </div>
                                </td>
                                <td class="order-price"><%= item.price %>$</td>
                                <td><button>Details</button></td>
                            </tr>
                        <% }) %>
                    </table>
                </div>
            </div>


        </div>

