<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" type="text/css" href="dashboard.css">
    <script src="/scripts/dashboard.js" defer></script>
</head>
<body>
    
    <!-- Modal -->

    <div class="modal-bg off"></div>

    <div class="info-modal modal off">
        Item successfully deleted
    </div>

    <div class="list-of-users-modal modal off">

        <h3>List of users</h3>

        <div class="list-of-users">

            <div class="list-header">
                <div class="h-id">Id</div>
                <div class="h-name">Name</div>
                <div class="h-date">Birth date</div>
            </div>
            
            <% allUsers.forEach((user,i) =>{ %>

                <% 
                    let shortName = user.name + ' ' + user.lastName
                    if(shortName.length > 10){
                        shortName = shortName.slice(0,10) + '...'
                    }
                %>
                <div class="user-cont">
                    <div class="user-id"><%= i+1 %></div>
                    <div class="user-name" title='<%= user.name + ' ' + user.lastName%>'><%= shortName %></div>
                    <div class="user-date"><%= user.date %></div>
                </div>

            <% }) %>

        </div>

    </div>
    
    <form class="add-product-modal modal off" method="POST" action="/addprod" enctype="multipart/form-data">

        <h3>Add product</h3>

        <label>
            <div class="i-txt">Name</div>
            <input type="text" class="" name="name">
        </label>

        <label>
            <div class="i-txt">Price</div>
            <input type="number" class="" name="price">
        </label>

        <label>
            <div class="i-txt">Description</div>
            <textarea class="description" name="description"></textarea>
        </label>

        <h4>Gender</h4>

        <label class="radio-cont">
            <input type="radio" name="gender" value="male">
            <div>Male</div>
        </label>

        <label class="radio-cont">
            <input type="radio" name="gender" value="female">
            <div>Female</div>
        </label>


        <h4>Colors</h4>
        <div class="colors-choose">
            <div class="color-c white" data-color="white"></div>
            <div class="color-c black" data-color="black"></div>
            <div class="color-c blue" data-color="blue"></div>
            <div class="color-c purple" data-color="purple"></div>
            <div class="add-color">+</div>
        </div>

        <h4>Sizes</h4>
        <div class="sizes-choose">
            <div class="size-c">S</div>
            <div class="size-c">M</div>
            <div class="size-c">L</div>
            <div class="size-c">XL</div>
        </div>

        <h4>Main image</h4>
        <input type="file" class="mainimg" accept="image/*" name="mainimg">
        <div class="mainImgText imgtxt"></div>
        
        <h4>Side images</h4>
        <input type="file" class="sideImgs" accept="image/*" multiple>
        <div class="sideImgsText imgtxt"></div>

        <button>Add product</button>

        <input type="hidden" class="color-inp" name="colours" value="">
        <input type="hidden" class="size-inp" name="sizes" value="">
    </form>




    <!-- Delete Product Modal -->
    <div class="delete-product-modal modal off">

        <label>
            <div>Product Code</div>
            <input type="text" name="delete-item" class="delete-item-inp">
        </label>

        <button class="delete-item">Delete</button>
    </div>



